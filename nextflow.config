process {
    cache = true
    executor = 'pbs'
    queue = 'batch'
    stageInMode = 'symlink'
    stageOutMode = 'rsync'
}
process.$runSTAR_process.container = 'containers/phelelani-nf-rnaSeqCount-master-star.simg'
process.$runHTSeqCount_process.container = 'containers/phelelani-nf-rnaSeqCount-master-htseqcount.simg'
process.$runFeatureCounts_process.container = 'containers/phelelani-nf-rnaSeqCount-master-featurecounts.simg'
process.$runMultiQC_process.container = 'containers/phelelani-nf-rnaSeqCount-master-multiqc.simg'
singularity {
    enabled = true
    cacheDir = 'containers'
    runOptions = '--bind /global/blast:/global/blast --bind /spaces:/spaces --cleanenv'
    autoMounts = false
}
trace {
    enabled = true
    file = 'report_workflow/nf-rnaSeqCount_trace.txt'
}
timeline {
    enabled = true
    file = 'report_workflow/nf-rnaSeqCount_timeline.html'
}
report {
    enabled = true
    file = 'report_workflow/nf-rnaSeqCount_report.html'
}
dag {
    enabled = true
    file = 'report_workflow/nf-rnaSeqCount.dot'
}
