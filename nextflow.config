process {
    cache = true
    executor = 'pbs'
    queue = 'batch'
    stageInMode = 'symlink'
    stageOutMode = 'rsync'
    clusterOptions = '#PBS -l nodes=n03.core.wits.ac.za-0+n05.core.wits.ac.za-0+n09.core.wits.ac.za-0+n13.core.wits.ac.za-0+n18.core.wits.ac.za-0+n19.core.wits.ac.za-0+n22.core.wits.ac.za-0+n24.core.wits.ac.za-0+n25.core.wits.ac.za-0+n26.core.wits.ac.za-0'
}
process.$runSTAR_process.container = 'containers/phelelani-rnaSeqCount-master-star.simg'
process.$runHTSeqCount_process.container = 'containers/phelelani-rnaSeqCount-master-htseqcount.simg'
process.$runFeatureCounts_process.container = 'containers/phelelani-rnaSeqCount-master-featurecounts.simg'
process.$runMultiQC_process.container = 'containers/phelelani-rnaSeqCount-master-multiqc.simg'
singularity {
    enabled = true
    cacheDir = 'containers'
    runOptions = '--bind /global/blast:/global/blast --bind /spaces:/spaces --cleanenv'
    autoMounts = false
}
trace {
    enabled = true
    file = 'report_workflow/rnaSeqCount_trace.txt'
}
timeline {
    enabled = true
    file = 'report_workflow/rnaSeqCount_timeline.html'
}
report {
    enabled = true
    file = 'report_workflow/rnaSeqCount_report.html'
}
dag {
    enabled = true
    file = 'report_workflow/rnaSeqCount.dot'
}
